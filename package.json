{
  "name": "@foxkit/eslint-config",
  "version": "1.1.0",
  "description": "Shareable ESLint config for foxkit-js packages",
  "main": "index.js",
  "repository": "git@github.com:foxkit-js/eslint-config.git",
  "author": "Mitsunee <michelle@mitsunee.com>",
  "license": "MIT",
  "eslintConfig": {
    "extends": "."
  },
  "devDependencies": {
    "clean-publish": "^4.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.0"
  },
  "peerDependencies": {
    "eslint": ">=7.32.0",
    "eslint-config-prettier": ">=8.3.0"
  },
  "files": [
    "rules.js",
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "tmp",
    "packageManager": "yarn",
    "fields": [
      "scripts",
      "packageManager"
    ],
    "files": [
      ".prettierrc.json"
    ]
  }
}
